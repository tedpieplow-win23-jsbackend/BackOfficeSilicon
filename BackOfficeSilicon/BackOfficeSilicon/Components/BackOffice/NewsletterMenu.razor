@rendermode InteractiveServer

@if (string.IsNullOrEmpty(EmailCheck))
{
    <NewsLettersView OnClick="RenderClickedPage"/>
}
else {
    <NewsletterEditForm OnClick="RenderClickedPage" Email="@EmailCheck" />
}

@code {
    [Parameter]
    public string? EmailIn { get; set; }
    private string EmailCheck = "";
    private void RenderClickedPage(string EmailIn)
    {
        EmailCheck = EmailIn;
        StateHasChanged();
    }
}
